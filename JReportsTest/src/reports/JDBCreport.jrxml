	<queryString language="SQL">
		<![CDATA[SELECT
  *
FROM (
  (
    SELECT
      CONCAT("Each ",category) AS Type,
      nom AS Machine,
      MIN(watt) AS Minimum,
      MAX(watt) AS Maximum,
      AVG(watt) AS Average
    FROM
      mesures
    GROUP BY
      nom
  ) UNION (
    SELECT
      CONCAT("All ",category, "s") AS Type,
      CONCAT(category, "s") AS Machine,
      MIN(watt) AS Minimum,
      MAX(watt) AS Maximum,
      AVG(watt) AS Average
    FROM
      mesures
    GROUP BY
      category
  )
) AS
  metrics
ORDER BY
  Type, Machine
;]]>
