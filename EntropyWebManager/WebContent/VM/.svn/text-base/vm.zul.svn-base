<window id="window" apply="fr.chavrier.entropyWebManager.composers.VMComposer" border="0" height="100%">
	<timer id="updateTimer" delay="1000" repeats="true">
    </timer>
	<borderlayout style="background: #FFFFFF">
		<north border="0" margins="5,5,5,5">
			<hbox style="border:0px">
				<button id="startBtn" label="start"></button>
				<button id="stopBtn" label="stop" ></button>
				<button id="suspendBtn" label="suspend" ></button>
				<button id="resumeBtn" label="resume" ></button>
				<button id="migrateBtn" label="migrate" ></button>
				<button id="deleteBtn" label="delete" ></button>
			</hbox>
		</north>
		<center id="center" border="0" margins="5,5,5,5" style="overflow:auto;">
			<div>
				<vbox width="805px">
					<hbox style="border:2px">
						<panel title="CPU perf." border="normal">
							<panelchildren>
								<hbox width="100%">
										<chart width="400" height="250" id="cpuChart" type="time_series">
										</chart>
								</hbox>
							</panelchildren>
						</panel>
						<panel title="Memory perfs." border="normal">
							<panelchildren>
								<hbox width="100%">
										<chart width="400" height="250" id="ramChart" type="time_series">
										</chart>
								</hbox>
							</panelchildren>
						</panel>
					</hbox>
					<panel title="Virtual machine infos" border="normal">
						<panelchildren>
							<grid style="border:0">
	                            <rows>
									<row>
	                                    <div>
	                                        <label id="nameLabel" />
	                                    </div>
	                                </row>
	                                <row>
	                                    <div>
	                                       <label id="stateLabel" />
	                                    </div>
	                                </row>
	                                <row>
	                                    <div>
	                                       <label id="ipLabel" />
	                                    </div>
	                                </row>
	                                <row>
	                                    <div>
	                                       <label id="nbCpuLabel" />
	                                    </div>
	                                </row>
	                                <row>
	                                    <div>
	                                        <label id="memorySizeLabel" />
	                                    </div>
	                                </row>
	                                <row>
	                                    <div>
	                                        <label id="usedCpuLabel" />
	                                    </div>
	                                </row>
	                                <row>
	                                    <div>
	                                        <label id="usedMemoryLabel" />
	                                    </div>
	                                </row>
	                                <row>
	                                    <div>
	                                        <label id="freeMemoryLabel" />
	                                    </div>
	                                </row>
								</rows>
							</grid>
						</panelchildren>
					</panel>
				</vbox>
			</div>
		</center>
	</borderlayout>
</window>