<tablelayout columns="3" height="100%" width="100%">
    <tablechildren colspan="3" height="10%">
        <panel border="normal" height="100%">
            <panelchildren>
                     <image id="logo" src="./Images/entropyLogo.jpg" width="100px" height="100px"/>
            </panelchildren>
        </panel>
    </tablechildren>
    <tablechildren  height="70%" width="20%">
        <panel border="normal" height="100%">
            <panelchildren>
                 <tree id="tree" height="100%" />
            </panelchildren>
         </panel>
    </tablechildren>
    <tablechildren colspan="2" height="70%">
        <panel border="normal" height="100%">
            <panelchildren>
               <tabbox height="100%" width="100%" id="tb" >
	                <tabs>
	                    <tab id="monitoredItem" label="Home" />
	                    <tab label="BtrScript Console" />
	                    <tab label="Entropy control" />
	                    <tab id="VNCTab" label="VNC Viewer" />
	                </tabs>
	                <tabpanels>
	                    <tabpanel>
	                        <include id="include" src="./error404.zul"/>
	                    </tabpanel>
	                    <tabpanel >
	                        <vlayout id="consoleOutput"  height="95%" width="100%" style="overflow:auto;"/>
	                        <textbox id="consoleInput" ctrlKeys="#up#down" height="4%" width="100%" />
	                    </tabpanel>
	                    <tabpanel>
	                        <panel title="Entropy mode" border="normal"
								style="margin-bottom:5px">
	                            <panelchildren>
	                                <radiogroup onCheck="">
	                                    <vbox>
	                                        <radio id="stopRadio" label="Stop" />
	                                        <radio id="semiRadio" label="Semi-auto" />
	                                        <radio id="autoRadio" label="Auto" />
	                                    </vbox>
	                                </radiogroup>
	                            </panelchildren>
	                        </panel>
	                        <timer id="timer" running="true" delay="1000"
								repeats="true">
	                        </timer>
	                        <panel title="Reconfiguration plan"
								border="normal">
	                            <panelchildren>
	                                <label id="planLabel" />
	                                <hbox>
	                                    <button id="notApplyBtn" label="Do NOT apply">
	                                    </button>
	                                    <button id="applyBtn" label="Apply">
	                                    </button>
	                                </hbox>
	                            </panelchildren>
	                        </panel>
	                    </tabpanel>
	                    <tabpanel id="vncViewer">
	                        <iframe id="guacamole" style="width: 100%; height: 100%; border: 0px;" />
	                    </tabpanel>
	                </tabpanels>
	            </tabbox>
            </panelchildren>
         </panel>
    </tablechildren>
    <tablechildren height="20%"  width="20%">
        <panel border="normal" height="100%">
            <panelchildren>
               
            </panelchildren> 
         </panel>
    </tablechildren>
    <tablechildren height="20%">
        <panel border="normal" height="100%">
            <panelchildren>
               <vlayout id="actionVLayout" width="100%" style="overflow:auto"/>
            </panelchildren> 
         </panel>
    </tablechildren>
    <tablechildren height="20%">
        <panel border="normal" height="100%">
            <panelchildren>
               <vlayout id="alertVLayout" width="100%" height="100%" style="overflow:auto"/>
            </panelchildren> 
         </panel>
    </tablechildren>
</tablelayout>